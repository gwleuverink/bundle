{
    "name": "leuverink/bundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Willem Leuverink",
            "email": "willem@leuver.ink"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/gwleuverink"
        }
    ],
    "autoload": {
        "psr-4": {
            "Leuverink\\Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leuverink\\Bundle\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9|^10",
        "illuminate/support": "^9|^10"
    },
    "require-dev": {
        "pestphp/pest": "^2.30",
        "symfony/thanks": "^1.2",
        "orchestra/testbench": "^8.19"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "symfony/thanks": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leuverink\\Bundle\\ServiceProvider"
            ]
        },
        "thanks": [
            {
                "name": "Bun",
                "url": "https://github.com/oven-sh/bun"
            }
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve"
        ],
        "test": [
            "@php vendor/bin/pest"
        ]
    }
}
